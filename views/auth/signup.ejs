<h2>sign up</h2>
<form action="/auth/signup" method="POST">
  <div class="row">
    <div class="input-field col s12 m6">
    <label for="email">Email</label>
    <input type="email" id="email" name="email" class="validate">
    </div>
    <div class="input-field col s12 m6">
    <label for="password">Password</label>
    <input type="password" id="password" name="password" class="validate">
    </div>
  </div>
  <div class="row">
    <div class="input-field col s12 m3">
    <label for="firstname">First Name</label>
    <input type="text" id="firstname" name="firstname" class="validate">
    </div>
    <div class="input-field col s12 m3">
    <label for="lastname">Last Name</label>
    <input type="text" id="lastname" name="lastname" class="validate">
    </div>
    <div class="input-field col s12 m3">
    <label for="dob">Birthday</label>
    <input type="text" id="dob" name="dob" class="datepicker validate">
    </div>
    <div class="input-field col s12 m3">
    <label for="image">Picture Url</label>
    <input type="url" id="image" name="image" class="validate">
    </div>
    <div class="input-field col s12 m3">
    <label for="weight">Weight(lb)</label>
    <input type="number" id="weight" name="weight" class="validate">
    </div>
    <div class="input-field col s12 m3">
    <label for="height">Height(in)</label>
    <input type="number" id="height" name="height" class="validate">
    </div>
  </div>
  <button class="btn waves-effect waves-light" type="submit" name="action">
  <i class="material-icons left">fitness_center</i><i class="material-icons right">send</i>Submit
</button>
</form>
